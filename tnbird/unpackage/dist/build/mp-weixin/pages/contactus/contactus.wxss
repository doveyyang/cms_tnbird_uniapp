.contact-container {
  background-color: #ffffff;
  position: relative;
}
.contact-container .banner, .contact-container image {
    height: 500rpx;
    width: 100%;
}
.contact-container .team {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    -webkit-flex-direction: column;
        -ms-flex-direction: column;
            flex-direction: column;
    margin: 20rpx;
    margin-top: 60rpx;
    background-color: #ffffff;
    border-radius: 20rpx;
    padding: 20rpx;
}
.contact-container .team .title {
      text-align: center;
      font-size: 32rpx;
      color: #0eb2fe;
      margin: 20rpx;
      font-weight: 700;
}
.contact-container .team .desc {
      font-size: 28rpx;
      color: #333;
      text-indent: 2em;
}
.contact-container .team .pic {
      margin-top: 20rpx;
}
.contact-container .values {
    height: 1150rpx;
}
.contact-container .info {
    padding: 20rpx;
    border-bottom: 1px solid #c8c7cc;
    margin-bottom: 50rpx;
}
.contact-container .info .title {
      margin: 40rpx;
      text-align: center;
      font-size: 32rpx;
      color: #333;
      font-weight: 700;
}
.contact-container .info .cards {
      background-color: #ffffff;
      border-radius: 20rpx;
      padding: 20rpx;
      position: relative;
}
.contact-container .info .cards .card {
        position: relative;
}
.contact-container .info .cards .card .info .name {
          font-size: 32rpx;
          display: -webkit-box;
          display: -webkit-flex;
          display: -ms-flexbox;
          display: flex;
          font-weight: 700;
          line-height: 35rpx;
          -webkit-box-align: baseline;
          -webkit-align-items: baseline;
              -ms-flex-align: baseline;
                  align-items: baseline;
          margin-bottom: 20rpx;
}
.contact-container .info .cards .card .info .name .name-desc {
            font-size: 28rpx;
            color: #999;
            margin-left: 14rpx;
            line-height: 20rpx;
}
.contact-container .info .cards .card .info .phone {
          font-size: 28rpx;
          display: -webkit-box;
          display: -webkit-flex;
          display: -ms-flexbox;
          display: flex;
          margin-bottom: 20rpx;
          line-height: 20rpx;
          -webkit-box-align: baseline;
          -webkit-align-items: baseline;
              -ms-flex-align: baseline;
                  align-items: baseline;
}
.contact-container .info .cards .card .info .phone .phone-desc {
            font-size: 28rpx;
            color: #0eb2fe;
            margin-left: 14rpx;
}
.contact-container .info .cards .card .info .email {
          font-size: 28rpx;
          display: -webkit-box;
          display: -webkit-flex;
          display: -ms-flexbox;
          display: flex;
          margin-bottom: 20rpx;
          line-height: 20rpx;
          -webkit-box-align: baseline;
          -webkit-align-items: baseline;
              -ms-flex-align: baseline;
                  align-items: baseline;
}
.contact-container .info .cards .card .info .email .email-desc {
            font-size: 28rpx;
            color: #0eb2fe;
            margin-left: 14rpx;
}
.contact-container .info .cards .card .info .address {
          font-size: 24rpx;
          margin-bottom: 20rpx;
          line-height: 20rpx;
          color: #999;
}
.contact-container .info .cards .card .avertar {
          position: absolute;
          width: 150rpx;
          height: 150rpx;
          right: 35rpx;
          top: 35rpx;
          border: 1px solid #0eb2fe;
          border-radius: 50%;
}
.contact-container .info .cards .card .avertar image {
            width: 130rpx;
            height: 130rpx;
            vertical-align: middle;
            margin: 10rpx;
}
.contact-container .callphone {
    position: fixed;
    padding: 30rpx;
    -webkit-box-sizing: border-box;
            box-sizing: border-box;
    bottom: 0;
    width: 100%;
    background-color: #ffffff;
    text-align: center;
}

